import java.io.BufferedReader;
import java.io.FileNotFoundException;
import java.io.FileReader;
import java.io.IOException;

public class MagicTrick {

	public static void main(String[] args) {
		solveProblem();
	}

	private static void solveProblem() {
		try {
			BufferedReader reader = new BufferedReader(
					new FileReader("input_magic_trick.txt"));
			String line = null;
			int counter = -1;
			int num_cases = 0;
			int first_row = 0;
			int second_row = 0;
			String row1 = "";
			String row2 = "";
			boolean first_matrix = false;
			int matrix_counter = 1;
			try {
				while ((line = reader.readLine()) != null) {
					if(counter == -1) {
						num_cases = Integer.parseInt(line);
						counter++;
					}
					if (counter == 10) {
						counter = 0;
					}
					if (counter == 0) {
						first_row = Integer.parseInt(line);
						matrix_counter = 1;
						counter++;
					}
					if (counter == 5) {
						second_row = Integer.parseInt(line);
						matrix_counter = 1;
						counter++;
					}
					else {
						if (first_matrix) {
							if (matrix_counter == first_row) {
								row1 = line;
								matrix_counter++;
							}
							else {
								matrix_counter++;
							}
						}
						else {
							if (matrix_counter == second_row) {
								row2 = line;
								matrix_counter++;
							}
							else {
								matrix_counter++;
							}
						}
					}
				}
				System.out.println(num_cases);
				System.out.println(first_row);
				System.out.println(row1);
				System.out.println(second_row);
				System.out.println(row2);
			} catch (IOException e) {
				e.printStackTrace();
			}
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		}
		
	}

}
